{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "public/index.html"
    }
  ]
}